    textHint: 
    mappingList: []
    allLanguage: 0
    isMapping: 0
    textSimpleChinese: "\u4E00\u5982\u65E2\u5F80\u5730\u654F\u9510\uFF0C\u6307\u6325\u5B98\u3002"
