    mappingList:
    - mapping/character/mapping_character_702
    allLanguage: 0
    isMapping: 1
    textSimpleChinese: "\u8BF7\u5141\u8BB8\u6211\u6B63\u5F0F\u4ECB\u7ECD\u4E00\u4E0B\u81EA\u5DF1\uFF1A\u6211\u662F{0}\u3002"
