  - key: loc_plot_c002_03_07_option_2
    textEnglish: What did you do to her?
    textHint: 
    mappingList: []
    allLanguage: 0
    isMapping: 0
    textSimpleChinese: "\u4F60\u5BF9\u5979\u505A\u4E86\u4EC0\u4E48\uFF1F"
    textTraditionChinese: "\u4F60\u5C0D\u5979\u505A\u4E86\u4EC0\u9EBC\uFF1F"
