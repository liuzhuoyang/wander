  - key: loc_plot_c002_03_04_option_2
    textEnglish: "You hacked her system back then, didn\u2019t you?"
    textHint: 
    mappingList: []
    allLanguage: 0
    isMapping: 0
    textSimpleChinese: "\u4F60\u90A3\u65F6\u5019\u9ED1\u8FDB\u4E86\u5979\u7684\u7CFB\u7EDF\uFF0C\u5BF9\u5427\uFF1F"
