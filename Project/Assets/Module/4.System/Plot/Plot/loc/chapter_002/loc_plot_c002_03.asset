  - key: loc_plot_c002_03_04_option_1
    textEnglish: That distress signal.
    textHint: 
    mappingList: []
    allLanguage: 0
    isMapping: 0
    textSimpleChinese: "\u90A3\u4E2A\u6C42\u6551\u4FE1\u53F7\u3002"
